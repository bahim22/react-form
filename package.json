        "copy-webpack-plugin": "^10.2.4",
        "css-loader": "^6.7.1",
        "css-minimizer-webpack-plugin": "^3.4.1",
        "cssnano": "^5.1.7",
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "file-loader": "^6.2.0",
        "html-loader": "^3.1.0",
