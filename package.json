	"description": "Hima Dionysus Balde React.js Portfolio Website that highlights my coding journey thus far.",
	"scripts": {
		"lint:pretest": "eslint \"src/**/*.{js,jsx}\"",
		"lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix",
		"prettier:check": "prettier --check \"src/**/*.{js,jsx}\"",
		"prettier:write": "npx prettier --write \"src/**/*.{js,jsx}\"",
		"test:espretty": "npx eslint-config-prettier src/components/*.js",
		"test:espretty1": "npx eslint-config-prettier \"src/**/*.{js,jsx}\"",
		"test:espretty2": "npx eslint-config-prettier App.js components/About.js components/FormPro.js",
		"test:lint": "eslint \"src/**/*.{js,jsx}\"",
		"test:lint-rules": "eslint src/components/*.js --config .eslintrc.json",
		"clean": "rm dist/*",
		"test:webpack": "npx webpack --config webpack.dev.config.js",
		"test:webpack-pro": "NODE_ENV=production npx webpack --config webpack.prod.config.js",
		"build": "npx webpack build --config webpack.prod.config.js --mode production",
		"dev": "npx webpack serve --config webpack.dev.config.js --mode development",
		"test:jest": "jest",
		"cypress:open": "cypress open"
	},
