{
	"plugins": [
		"@babel/plugin-syntax-dynamic-import",
		"@babel/plugin-proposal-class-properties",
		[
			"@babel/plugin-transform-runtime",
			{
				"corejs": 3,
				"proposals": true,
				"absoluteRuntime": true,
				"helpers": true,
				"regenerator": true,
				"version": "^7.17.0"
				//** can be 2 | 3 && proposals: tru
			}
		]
		// "@babel/plugin-transform-runtime",
		// "@babel/plugin-transform-react-jsx"
    ],
	"targets": {
		"browsers": [
			"> .2%",
			"not ie <= 10"
		],
		"node":"12.0.0",
		"esmodules": true
	},
	"parserOpts": {
		"plugins": [
			"jsx",
			// "flow"
		]
	},
	"generatorOpts": {
		"minified": true,
		"compact": true
	},
	"sourceType": "unambiguous",
	"presets": [
		[
			"@babel/preset-env",
			{
				"modules": "auto",
				"bugfixes": true,
				"useBuiltIns": "usage",
				"corejs": 3
			}
		],
        [
			"@babel/preset-react",
			{
				"runtime": "automatic"
			}
		]
    ]
}
