{
    "plugins": ["react", "import", "jsx-a11y", "prettier"],
	"extends": [
		"eslint:recommended",
		"plugin:import/errors",
		"plugin:react/recommended",
		"plugin:jsx-a11y/recommended"
		// "plugin:prettier/recommended"
	],
	"ignorePatterns": ["node_modules/", "dist/", "build/", "webpack.*.js"],
	// "module": true,
	"parserOptions": {
		"ecmaVersion": 2021,
		// "allowImportExportEverywhere": true,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
		// "development": true,
		// "module": true
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
