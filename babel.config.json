{
	"plugins": [
		"@babel/plugin-syntax-dynamic-import",
		"@babel/plugin-transform-react-jsx",
		"@babel/plugin-proposal-class-properties",
		[
			"@babel/plugin-transform-runtime",
			{
				"absoluteRuntime": false,
				"corejs": 3, //** can be 2 | 3 && proposals: true
				"helpers": true,
				"regenerator": true,
				"version": "^7.17.0"
			}
		]
    ],
	"targets": {
		"browsers": [
			"last 2 Chrome versions",
			"last 2 Firefox versions",
			"last 2 Safari versions",
			"last 2 Edge versions",
			"last 2 iOS versions",
			"last 1 ChromeAndroid version",
			"last 1 Android version",
			"ie 11"
		],
		"node":"12.0.0",
		"esmodules": true
	},
	"parserOpts": {
		"plugins": [
			"jsx",
			"flow"
		]
	},
	// "generatorOpts": {
	// 	"minified": true,
	// 	"compact": true
	// },
	"sourceType": "unambiguous",
	"presets": [
		[
			"@babel/preset-env",
			{
				"modules": "auto",
				"bugfixes": true,
				"useBuiltIns": "usage",
				"corejs": 3
			}
		],
        [
			"@babel/preset-react",
			{
				"runtime": "automatic"
			}
		]
    ]
}
