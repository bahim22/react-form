{
	"plugins": [
		"@babel/plugin-syntax-dynamic-import",
		"@babel/plugin-transform-react-jsx",
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-transform-runtime"
		// [
		// 	"@babel/plugin-transform-runtime",
		// 	{
		// 		"absoluteRuntime": true,
		// 		"corejs": 3, //** can be 2 | 3 && proposals: true
		// 		"helpers": true,
		// 		"regenerator": true,
		// 		"version": "^7.17.0"
		// 	}
		// ]
    ],
	"targets": {
		"browsers": [
			"> 1%",
			"last 2 versions",
			"not ie <= 10",
			"not dead"
		],
		"node":"12.0.0",
		"esmodules": true
	},
	"parserOpts": {
		"plugins": [
			"jsx",
			"flow"
		]
	},
	// "generatorOpts": {
	// 	"minified": true,
	// 	"compact": true
	// },
	"sourceType": "unambiguous",
	"presets": [
		[
			"@babel/preset-env",
			{
				"modules": "auto",
				"bugfixes": true,
				"useBuiltIns": "usage",
				"corejs": 3
			}
		],
        [
			"@babel/preset-react",
			{
				"runtime": "automatic"
			}
		]
    ]
}
